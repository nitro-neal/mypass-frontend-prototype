  "main": "index.js",
  "scripts": {
    "start": "npx webpack-dev-server --mode=development --colors --config webpack.config.dev.ts",
    "build": "npm run clean-dist && npx webpack --mode production --config webpack.config.prod.ts",
    "deploy": "npm run build && aws s3 sync dist/ s3://mypass-atx --delete --acl public-read",
    "test": "npx jest --config=jest.config.json",
    "ts-build": "npx tsc",
    "lint": "npx tslint -c tslint.json -p tsconfig.json",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "npx rimraf ./dist",
    "zep-connect": "npx zeplin connect",
    "zep-react": "npx zeplin connect -p @zeplin/cli-connect-react-plugin"
  },
